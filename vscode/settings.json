{
  "code-runner.runInTerminal": true,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "debug.onTaskErrors": "abort",
  "editor.accessibilityPageSize": 50,
  "diffEditor.maxFileSize": 0,
  "window.restoreFullscreen": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "kotlin.languageServer.enabled": false,
  "kotlin.debugAdapter.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "afterDelay",
  "materialTheme.accent": "Purple",
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontWeight": "330",
  "window.title": "${folderPath}",
  "editor.fontSize": 14,
  "workbench.startupEditor": "none",
  "editor.cursorBlinking": "expand",
  "editor.formatOnSave": true,
  "git.enableSmartCommit": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.env.linux": {},
  "console-ninja.allowedHosts": ["http://127.0.0.1:5500/"],
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "editor.lineNumbers": "relative",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.colorTheme": "Catppuccin Macchiato",
  "settingsSync.ignoredExtensions": [],
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "editor.cursorSurroundingLines": 1000,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "window.zoomLevel": -1,
  "tabnine.experimentalAutoImports": true,
  "liveServer.settings.host": "localhost",
  // "liveServer.settings.host": "",
  "liveServer.settings.root": "",
  "tailwind-fold.autoFold": false,
  "hediet.vscode-drawio.resizeImages": null,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.sideBar.location": "right",
  "prisma.showPrismaDataPlatformNotification": false,
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  }
  // "window.titleBarStyle": "custom",
}
